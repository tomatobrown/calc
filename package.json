{
    "name": "calc",
    "version": "1.0.0",
    "description": "A simple JavaScript project with unit tests",
    "scripts": {
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "devDependencies": {
        "coveralls": "^3.1.1",
        "jest": "^27.5.1",
        "jest-cli": "^29.7.0"
    },
    "jest": {
        "coverageReporters": [
            "lcov"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.js"
        ],
        "testTimeout": 30000
    }
}